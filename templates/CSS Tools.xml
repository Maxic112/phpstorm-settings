<templateSet group="CSS Tools">
  <template name="snippetPropNoSelect" value="-webkit-touch-callout: none; /* iOS Safari */&#10;-webkit-user-select: none; /* Safari */&#10;-khtml-user-select: none; /* Konqueror HTML */&#10;-moz-user-select: none; /* Firefox */&#10;-ms-user-select: none; /* Internet Explorer/Edge */&#10;user-select: none; /* Non-prefixed version, currently supported by Chrome and Opera */" description="Properties that forbid the selection of an element" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="@msr" value="@media screen and (max-width: $WIDTH$) {&#10;&#10;&#9;$END$&#10;}" description="@media screen and (max-width: ?px)" toReformat="false" toShortenFQNames="true">
    <variable name="WIDTH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="meds" value="@media screen and (max-width: $WIDTH$) {&#10;&#10;&#9;$END$&#10;}" description="@media screen and (max-width: 1px)" toReformat="false" toShortenFQNames="true">
    <variable name="WIDTH" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="snippetSelectorPlaceholder" value="$SELECTOR$::placeholder {&#10;&#9;$START$&#10;}&#10;&#10;$SELECTOR$:-ms-input-placeholder {&#10;&#9;$START$&#10;}&#10;&#10;$SELECTOR$::-ms-input-placeholder {&#10;&#9;$START$&#10;}&#10;&#10;$SELECTOR$::-webkit-input-placeholder {&#10;&#9;$START$&#10;}&#10;&#10;$SELECTOR$:-moz-placeholder {&#10;&#9;$START$&#10;}&#10;&#10;$SELECTOR$::-moz-placeholder {&#10;&#9;$START$&#10;}" description="" toReformat="false" toShortenFQNames="true">
    <variable name="SELECTOR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="START" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="dflex" value="display: -webkit-box;&#10;display: -moz-box;&#10;display: -ms-flexbox;&#10;display: -webkit-flex;&#10;display: flex;" description="display: flex;" toReformat="false" toShortenFQNames="true" deactivated="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="snTableBase" value="width: 100%;&#10;border-collapse: collapse;&#10;border: 1px solid black;&#10;$END$" description="Base Property for Table" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="bgim" value="background-image: url('$END$');&#10;background-position: $position$;&#10;background-size: $size$;&#10;background-repeat: $repeat$;" description="background-image Template" toReformat="false" toShortenFQNames="true">
    <variable name="position" expression="" defaultValue="center" alwaysStopAt="true" />
    <variable name="size" expression="" defaultValue="100%" alwaysStopAt="true" />
    <variable name="repeat" expression="" defaultValue="no-repeat" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
  <template name="snf" value="@include fw('$1$');&#10;font-size: $2$px;&#10;line-height: ($3$/$2$);&#10;color: $4$;" description="Snippet Font Property" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="3" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="4" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS" value="true" />
    </context>
  </template>
</templateSet>